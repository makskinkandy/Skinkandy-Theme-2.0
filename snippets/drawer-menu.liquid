{%- assign animation_row = 1 -%}
{%- assign drawer_position = 'right' -%}
{% if logo_alignment == 'center' or logo_alignment == 'left' %}
  {%- assign drawer_position = 'left' -%}
{% endif %}
{% assign au_contact = section.settings.call_us_link_au%}
{% assign au_contact_label = section.settings.call_us_label_au %}
{% assign nz_contact = section.settings.call_us_link_nz%}
{% assign nz_contact_label = section.settings.call_us_label_nz %}

<div id="NavDrawer" class="drawer drawer--{{ drawer_position }}">
  <div class="drawer__contents">
    <div class="drawer__fixed-header">
      <div class="drawer__header appear-animation appear-delay-{{ animation_row }}">
        
        <button type="button" class="drawer__close-button js-drawer-close" aria-label="Close Button">
          <svg width="28" height="28" viewBox="0 0 28 28" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" aria-label="Close Menu">
            <rect width="28" height="28" fill="url(#pattern0_196_1246)"/>
            <defs>
            <pattern id="pattern0_196_1246" patternContentUnits="objectBoundingBox" width="1" height="1">
            <use xlink:href="#image0_196_1246" transform="scale(0.00195312)"/>
            </pattern>
            <image id="image0_196_1246" width="512" height="512" xlink:href="data:image/png;base64,"/>
            </defs>
          </svg>

        </button>
        {%- for block in section.blocks -%}
          {%- if block.type == 'logo' -%}
            <a
              href="{{ routes.root_url }}">
            <img
              src="{{ block.settings.logo_mobile | image_url }}"
              alt="{{ block.settings.logo_mobile.alt | default: shop.name }}">
            </a>
          {%- endif -%}
        {%- endfor -%}
      </div>
    </div>
    <div class="drawer__scrollable">
      <ul class="mobile-nav{% if settings.type_navigation_style == 'heading' %} mobile-nav--heading-style{% endif %}" role="navigation" aria-label="Primary">
       
        {%- for link in main_menu.links -%}
           
          {%- assign animation_row = animation_row | plus: 1 -%}
          {%- assign child_list_handle = link.url | handleize | append: forloop.index -%}
          {%- assign has_dropdown = false -%}
          {% assign link_handle = link.url | handle %}
          {%- if link.links != blank -%}
            {%- assign has_dropdown = true -%}
          {%- endif -%}

          {% assign childlink_title_dc = link.title | downcase %}

          <li class="mobile-nav__item appear-delay-{{ animation_row }} {% if link_handle == 'pages-store-locations' %}page-location{% endif %}">
            {%- if has_dropdown -%}
              <div class="mobile-nav__has-sublist">
                {%- if link.url == '#' or link.url == "/en-nz#"  -%}
                  <button type="button"
                    aria-controls="Linklist-{{ child_list_handle }}"
                    class="mobile-nav__link--button mobile-nav__link--top-level">
                    <span class="mobile-nav__faux-link"{% if link.active %} data-active="true"{% endif %}>
                      {{ link.title }}
                    </span>
                    <div class="mobile-nav__toggle">
                      <svg width="19" height="19" viewBox="0 0 19 19" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" aria-label="More options">
                        <path d="M0 0H19V19H0L0 0Z" fill="url(#pattern0_196_677)"/>
                        <defs>
                        <pattern id="pattern0_196_677" patternContentUnits="objectBoundingBox" width="1" height="1">
                        <use xlink:href="#image0_196_677" transform="scale(0.00195313)"/>
                        </pattern>
                        <image id="image0_196_677" width="512" height="512" xlink:href="data:image/png;base64,"/>
                        </defs>
                      </svg>

                    </div>
                  </button>
                {%- else -%}
                  <a href="{{ link.url }}"
                    class="mobile-nav__link mobile-nav__link--top-level {% if link.url contains 'shop-the-sale' %}sale-text{% endif %}"
                    id="Label-{{ child_list_handle }}"
                    {% if link.active %}data-active="true"{% endif %}>
                    {{ link.title }}
                  </a>
                  <div class="mobile-nav__toggle">
                    <button type="button"
                      aria-controls="Linklist-{{ child_list_handle }}"
                      aria-labelledby="Label-{{ child_list_handle }}"
                      class="">
                      <svg width="19" height="19" viewBox="0 0 19 19" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" aria-label="More options">
                        <path d="M0 0H19V19H0L0 0Z" fill="url(#pattern0_196_677)"/>
                        <defs>
                        <pattern id="pattern0_196_677" patternContentUnits="objectBoundingBox" width="1" height="1">
                        <use xlink:href="#image0_196_677" transform="scale(0.00195313)"/>
                        </pattern>
                        <image id="image0_196_677" width="512" height="512" xlink:href="data:image/png;base64,"/>
                        </defs>
                      </svg>

                    </button>
                  </div>
                {%- endif -%}
              </div>
            {%- else -%}
              <a href="{{ link.url }}" class="mobile-nav__link mobile-nav__link--top-level {% if link.url contains 'shop-the-sale' %}sale-text{% endif %}"{% if link.active %} data-active="true"{% endif %}>{{ link.title }}</a>
            {%- endif -%}

            {%- if has_dropdown -%}
              <div id="Linklist-{{ child_list_handle }}"
                class="mobile-nav__sublist"
                {% if link.active or link.child_active %}style="height: auto;"{% endif %}>
                <div class="collapsible-content__inner">
                  <ul class="level-2-list">
                      {%- for childlink in link.links -%}
                        {%- assign has_sub_dropdown = false -%}
                        {%- assign grand_child_list_handle = childlink.url | handleize | append: forloop.index -%}
                        {%- if childlink.links != blank -%}
                          {%- assign has_sub_dropdown = true -%}
                        {%- endif -%}
                        {% assign link_handle = childlink.url | handle %}
                        
                        <li class="main-menu-item">
                          <a href="{{ childlink.url }}"
                            class="mobile-nav__link {% if link_handle == 'products-sk-gift-card' %}highlight{% endif %} level-2-menu"
                            id="Sublabel-{{ grand_child_list_handle }}" aria-controls="Linklist-{{ child_list_handle }}"
                            {% if childlink.active %}data-active="true"{% endif %}>
                            {{ childlink.title | escape }}
                            {% if has_sub_dropdown %}
                              <svg width="19" height="19" viewBox="0 0 19 19" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" aria-label="More options">
                                <path d="M0 0H19V19H0L0 0Z" fill="url(#pattern0_196_677)"/>
                                <defs>
                                <pattern id="pattern0_196_677" patternContentUnits="objectBoundingBox" width="1" height="1">
                                <use xlink:href="#image0_196_677" transform="scale(0.00195313)"/>
                                </pattern>
                                <image id="image0_196_677" width="512" height="512" xlink:href="data:image/png;base64,"/>
                                </defs>
                              </svg>
                            {% endif %}
                          </a>
                          {% if childlink.links and has_sub_dropdown %}
                            <ul class="level-2-sub">
                              {%- for grandchildlink in childlink.links -%}
                                {% assign link_handle = grandchildlink.url | handle %}
                                <li>
                                  <a href="{{ grandchildlink.url }}" class="mobile-nav__link {% if link_handle == 'products-sk-gift-card' %}highlight{% endif %}">
                                    {{ grandchildlink.title }}
                                  </a>
                                </li>
                              {%- endfor -%}
                            </ul>
                            
                            
                          {% endif %}
                        </li>
                      {%- endfor -%}
                    </ul>
                </div>
              </div>
            {%- endif -%}
          </li>
        {%- endfor -%}

      </ul>
      {% comment %} Book Now {% endcomment %}

      {% if book_now_link %}
        <a href="{{ book_now_link }}" class="btn book-now">{{ book_now_label }}</a>
      {% endif %}

      {% if au_contact %}
        <div class="mobile-nav__contact">
          Prefer to speak with a person? <br>
          <div class="contact">
            <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="23" height="23" viewBox="0 0 23 23" fill="none" aria-hidden="true">
              <rect width="23" height="23" fill="url(#pattern0_196_722)"/>
              <defs>
              <pattern id="pattern0_196_722" patternContentUnits="objectBoundingBox" width="1" height="1">
              <use xlink:href="#image0_196_722" transform="scale(0.00195312)"/>
              </pattern>
              <image id="image0_196_722" width="512" height="512" xlink:href="data:image/png;base64,"/>
              </defs>
            </svg>
          {% if request.locale.root_url contains 'en-nz' %}
              CALL US: <a href="{{ nz_contact }}">{{ nz_contact_label }}</a>
          {% else %}
            CALL US: <a href="{{ au_contact }}">{{ au_contact_label }}</a>
          {% endif %}
          </div>
        </div>
      {% endif %}
      {% if section.settings.country != blank %}
        <div class="mobile-nav__country">
          <ul class="header-icons">
            <li class="dropdown">
              <button class="site-nav__link site-nav__link--icon global-icon" style="padding-left:5px !important;padding-right:10px !important;">
                {% if localization.country == 'Australia' %}
                  AU <svg xmlns="http://www.w3.org/2000/svg" aria-label="AU" width="16" height="16" fill="currentColor" class="bi bi-chevron-down" viewBox="0 0 16 16" role="presentation" aria-label="Select Country">\
                  <path fill-rule="evenodd" d="M1.646 4.646a.5.5 0 0 1 .708 0L8 10.293l5.646-5.647a.5.5 0 0 1 .708.708l-6 6a.5.5 0 0 1-.708 0l-6-6a.5.5 0 0 1 0-.708"/>\
                  </svg>
                {% else %}
                  NZ <svg xmlns="http://www.w3.org/2000/svg" aria-label="NZ" width="16" height="16" fill="currentColor" class="bi bi-chevron-down" viewBox="0 0 16 16" role="presentation" aria-label="Select Country">\
                  <path fill-rule="evenodd" d="M1.646 4.646a.5.5 0 0 1 .708 0L8 10.293l5.646-5.647a.5.5 0 0 1 .708.708l-6 6a.5.5 0 0 1-.708 0l-6-6a.5.5 0 0 1 0-.708"/>\
                  </svg>
                {% endif %}
              </button>
              <div class="dropdown-box" style="width: 57.93px;">
                    <a  style="cursor:pointer" id="link-au">AU</a>
                    <a  style="cursor:pointer" id="link-nz">NZ</a>
              </div>
            </li>
          </ul>
        </div>
      {% endif %}
    </div>

    <div class="drawer__footer">
      {%- assign animation_row = animation_row | plus: 1 -%}
        {% if about_label != blank %}
          <ul class="mobile-nav mobile-nav--heading-style">
            <li class="mobile-nav__item appear-delay-{{ animation_row }}">
              {% if about_nav != blank %}
                <div class="mobile-nav__has-sublist">
                  <button type="button"
                    aria-controls="Linklist-{{ child_list_handle }}"
                    class="mobile-nav__link--button mobile-nav__link--top-level collapsible-trigger collapsible--auto-height">
                    <span class="mobile-nav__faux-link">
                      {{ about_label }}
                    </span>
                    <div class="mobile-nav__toggle">
                      <svg xmlns="http://www.w3.org/2000/svg" width="8.848" height="15.652" viewBox="0 0 8.848 15.652" aria-hidden="true">
                        <path id="Path_2580" data-name="Path 2580" d="M3.162,7.827,8.505,2.009a1.182,1.182,0,0,0,0-1.664,1.157,1.157,0,0,0-1.648,0L.342,6.993a1.186,1.186,0,0,0,0,1.665l6.515,6.648a1.157,1.157,0,0,0,1.648,0,1.182,1.182,0,0,0,0-1.664Z" transform="translate(8.848 15.652) rotate(180)"/>
                      </svg>
                    </div>
                  </button>
                </div>

                <div id="Linklist-{{ child_list_handle }}"
                  class="mobile-nav__sublist collapsible-content collapsible-content--all"
                  {% if link.active or link.child_active %}style="height: auto;"{% endif %}>
                  <div class="collapsible-content__inner">
                    <button class="btn btn--small link-icon">
                      <div>
                        <svg xmlns="http://www.w3.org/2000/svg" width="8.848" height="15.652" viewBox="0 0 8.848 15.652" aria-hidden="true">
                          <path id="Path_2579" data-name="Path 2579" d="M10.82,13.226l5.343-5.818a1.182,1.182,0,0,0,0-1.664,1.157,1.157,0,0,0-1.648,0L8,12.392a1.186,1.186,0,0,0,0,1.665l6.515,6.648a1.157,1.157,0,0,0,1.648,0,1.182,1.182,0,0,0,0-1.664Z" transform="translate(-7.658 -5.399)"/>
                        </svg>
                        <span>back to main navigation</span>
                      </div>
                    </button>
                      <ul class="mobile-nav__sublist">
                        {%- for childlink in linklists[about_nav].links -%}
                          {%- assign has_sub_dropdown = false -%}
                          {%- assign grand_child_list_handle = childlink.url | handleize | append: forloop.index -%}
                          {%- if childlink.links != blank -%}
                            {%- assign has_sub_dropdown = true -%}
                          {%- endif -%}

                          <li class="mobile-nav__item">
                            <div class="mobile-nav__child-item">
                              {%- if childlink.url == '#' and has_sub_dropdown -%}
                                <button type="button"
                                aria-controls="Sublinklist-{{ child_list_handle }}-{{ grand_child_list_handle }}"
                                class="mobile-nav__link--button collapsible-trigger">
                                  <span class="mobile-nav__faux-link">{{ childlink.title | escape }}</span>
                                  {%- render 'collapsible-icons-alt' -%}
                                </button>
                              {%- else -%}
                                <a href="{{ childlink.url }}"
                                  class="mobile-nav__link"
                                  id="Sublabel-{{ grand_child_list_handle }}">
                                  {{ childlink.title | escape }}
                                </a>
                              {%- endif -%}
                              {%- if childlink.url != '#' and has_sub_dropdown -%}
                                <button type="button"
                                  aria-controls="Sublinklist-{{ child_list_handle }}-{{ grand_child_list_handle }}"
                                  aria-labelledby="Sublabel-{{ grand_child_list_handle }}"
                                  class="collapsible-trigger{% if childlink.active or childlink.child_active %} is-open{% endif %}">
                                  {%- render 'collapsible-icons-alt' -%}
                                </button>
                              {%- endif -%}
                            </div>

                            {%- if has_sub_dropdown -%}
                              <div
                                id="Sublinklist-{{ child_list_handle }}-{{ grand_child_list_handle }}"
                                aria-labelledby="Sublabel-{{ grand_child_list_handle }}"
                                class="mobile-nav__sublist collapsible-content collapsible-content--all"
                                {% if childlink.active or childlink.child_active %}style="height: auto;"{% endif %}>
                                <div class="collapsible-content__inner">
                                  <ul class="mobile-nav__grandchildlist">
                                    {%- for grandchildlink in childlink.links -%}
                                      <li class="mobile-nav__item">
                                        <a href="{{ grandchildlink.url }}" class="mobile-nav__link">
                                          {{ grandchildlink.title }}
                                        </a>
                                      </li>
                                    {%- endfor -%}
                                  </ul>
                                </div>
                              </div>
                            {%- endif -%}
                          </li>
                        {%- endfor -%}
                      </ul>
                  </div>
                </div>
              {%- else -%}
                <a href="{{ about_link}}" class="mobile-nav__link mobile-nav__link--top-level">{{ about_label }}</a>
              {% endif %}
            </li>
          </ul>
        {% endif %}
      {% comment %}
        <div class="appear-animation appear-delay-{{ animation_row }}">
          {%- render 'social-links', class: 'flex-grid--justify-end' -%}
        </div>
      {% endcomment %}
    </div>
  </div>
</div>
