    btnsMore[i].classList.remove('d-none');
    btn.classList.add('d-none');
    contents[i].classList.add('d-none');
    activeContentIndex = -1;
  };
});

document.addEventListener("DOMContentLoaded", () => {


  const buttons = document.querySelectorAll(".pf-anchor");
  document.documentElement.classList.add("no-scroll");
  Promise.all(
    Array.from(buttons).map((button) => {
      return new Promise((resolve) => {

        const event = new MouseEvent('click', {
          bubbles: true,
          cancelable: true,
          view: window,
        });
        button.dispatchEvent(event);

        setTimeout(() => {
          resolve();
        }, 1000);
      });
    })
  ).then(() => {
    document.documentElement.classList.remove("no-scroll");
  });

  const showButtons = document.querySelectorAll('.show-options');

  showButtons.forEach(button => {
    button.addEventListener('click', (event) => {
      const parentAccordion = event.target.closest('.parent-accordion');
      const accItems = parentAccordion.querySelectorAll('.acc-item');
      const isHidden = accItems[0].classList.contains('hide-acc');

      if (isHidden) {
        accItems.forEach(item => item.classList.remove('hide-acc'));
        button.textContent = 'HIDE OPTIONS';
      } else {
        accItems.forEach(item => item.classList.add('hide-acc'));
        button.textContent = 'SEE OPTIONS';
        button.scrollIntoView({
          behavior: 'smooth',
          block: 'start',
        });
      }
    });
  });

});</script>