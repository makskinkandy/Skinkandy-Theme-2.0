    "rich_text_bLWNfw": {
      "type": "rich-text",
      "blocks": {
        "heading_xTiWAV": {
          "type": "heading",
          "settings": {
            "title": "Bendable Rings (Seamless Rings)",
            "heading_tag": "h2"
          }
        },
        "text_bD4CDe": {
          "type": "text",
          "settings": {
            "enlarge_text": false,
            "text": "<p><strong>How it works:</strong><br/><br/>A single piece of metal that you gently twist open sideways, insert into your piercing, then twist back to close.<br/></p><p><strong>Why it’s great:</strong></p><ul><li>Minimalist, seamless design.</li><li>Lightweight and comfortable.</li><li>Budget-friendly.</li></ul><p><strong>Things to consider: </strong></p><p>They can lose shape if over-bent and are trickier to change than hinged rings, so they’re best for healed piercings.</p><p><strong>Best for:</strong> Nostril, lobe, or helix piercings where a clean, subtle look is key.</p>"
          }
        }
